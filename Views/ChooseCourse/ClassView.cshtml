@model AdaptiveLearningFinal.Models.LearningModel

@{
    ViewBag.Title = "ClassView";
}

<h2>
   @Model.SelectedItem
</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>


@using (Html.BeginForm("Index", "ShowCourseTest", FormMethod.Get)) {
    @Html.ValidationSummary(true)
    @Html.HiddenFor(x=>x.SelectedItem)
    <div>
        <iframe type="text/html" width="1028" height="764" src="@ViewBag.MaterialID" frameborder="0"></iframe>

    </div>
        
    <p>
        <input type="submit" value="Take Test" />
    </p>
}